<?xml version="1.0" encoding="utf-8"?>
<network-security-config>
    <!-- Allow cleartext traffic for model downloads -->
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">models.thinq.ai</domain>
    </domain-config>
    
    <!-- Alternatively, to allow all HTTP traffic (less secure): -->
    <!-- <base-config cleartextTrafficPermitted="true" /> -->
</network-security-config>
